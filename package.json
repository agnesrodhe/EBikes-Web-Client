{
  "name": "webbapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@react-google-maps/api": "^2.17.0",
    "@react-oauth/google": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.2.1",
    "chart.js": "^4.0.1",
    "enzyme": "^3.11.0",
    "eventsource": "^2.0.2",
    "google-map-react": "^2.2.0",
    "js-cookie": "^3.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.1",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-icons": "^4.6.0",
    "react-loading-icons": "^1.1.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "recharts": "^2.1.16",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --watchAll=false --coverage --collectCoverageFrom=!src/reportWebVitals.js --collectCoverageFrom=!src/index.js",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules package-lock.json",
    "eslint": "eslint .",
    "posttest": "npm run eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "enableDangerousAutofixThisMayCauseInfiniteLoops": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "javascript-style-guide": "^1.0.1",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/(?!chartjs)/(?!react-chartjs-2)"
    ]
  }
}
